{"ast":null,"code":"import PxAlert from '@/components/PxAlert';\nimport PxPaginacion from '@/components/PxPaginacion';\nimport PxDetalle from '@/components/PxDetalle';\nimport PxFooter from '@/components/PxFooter';\nimport app from \"@/app\";\nexport default {\n  name: 'App',\n  components: {\n    PxAlert,\n    PxPaginacion,\n    PxDetalle,\n    PxFooter\n  },\n  data() {\n    return {\n      // isLoading: false,\n      asset: {},\n      pokemonID: \"\",\n      cant: 1,\n      message: false,\n      message_increment: false,\n      text_increment: \"es el ultimo pokemon de la lista. No se puede seguir hacia adelante. Seleccione el botón anterior.\",\n      text: \"es el ultimo pokemon de la lista. No se puede seguir hacia atrás. Seleccione el botón siguiente.\",\n      isLoading: false\n    };\n  },\n  created() {\n    this.getCoin();\n  },\n  methods: {\n    increment() {\n      this.message = false;\n      this.message_increment = false;\n      this.cant = this.cant + 1;\n      const imprimir = this.cant;\n      console.log(imprimir);\n      if (this.cant > 8) {\n        this.cant = 8;\n        this.message_increment = true;\n      }\n      if (this.cant > 0) {\n        this.getCoin(this.cant);\n      }\n    },\n    decrement() {\n      this.message_increment = false;\n      this.cant = this.cant - 1;\n      const imprimir = this.cant;\n      console.log(imprimir);\n      if (this.cant < 1) {\n        this.cant = 1;\n        this.message = true;\n      }\n      if (this.cant > 0) {\n        this.getCoin(this.cant);\n      }\n    },\n    getCoin() {\n      this.isLoading = true;\n      Promise.all([app.getPokemon(this.cant)]).then(([asset]) => {\n        this.asset = asset;\n        // this.history = history;\n      }).finally(() => this.isLoading = false);\n    },\n    getBusqueda() {\n      this.isLoading = true;\n      Promise.all([app.getPokemon(this.pokemonID)]).then(([asset]) => {\n        this.asset = asset;\n        // this.history = history;\n      }).finally(() => this.isLoading = false);\n    }\n  }\n};","map":{"version":3,"mappings":"AAoDA;AACA;AACA;AAEA;AACA;AACA;EACAA;EAEAC;IACAC;IAAAC;IAAAC;IAAAC;EACA;EACAC;IACA;MACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IAEA;EAEA;EACAC;IACA;EACA;EAEAC;IACAC;MAEA;MACA;MACA;MACA;MACAC;MAEA;QACA;QACA;MACA;MACA;QACA;MACA;IACA;IACAC;MAEA;MACA;MACA;MACAD;MACA;QACA;QACA;MACA;MACA;QACA;MACA;IAEA;IACAE;MACA;MACAC,yCACAC;QACA;QACA;MACA,GACAC;IACA;IACAC;MACA;MACAH,8CACAC;QACA;QACA;MACA,GACAC;IACA;EACA;AAEA","names":["name","components","PxAlert","PxPaginacion","PxDetalle","PxFooter","data","asset","pokemonID","cant","message","message_increment","text_increment","text","isLoading","created","methods","increment","console","decrement","getCoin","Promise","then","finally","getBusqueda"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-main>\n      <form class=\"pokemon-form\">\n        <label for=\"input\" class=\"pokemon-label\"><h1>Busca por nombre o número </h1></label>\n        <div>\n\n          <input\n\n            type=\"text\"\n            name=\"pokemon\"\n            v-model=\"pokemonID\"\n            placeholder=\"Busca tu pokemon\"\n            autocomplete=\"off\"/>\n\n            <v-btn @click=\"getBusqueda\" class=\"ma-2\" color=\"black darken-2\" dark>\n              Buscar pokemon \n            </v-btn>\n          </div>\n\n\n      </form>\n\n      <PxPaginacion  v-bind:increment=\"increment\" v-bind:decrement=\"decrement\" v-bind:cant=\"cant\"/>\n      <div class=\"alinear\">\n\n      <div class=\"content_alert\">\n        <div v-if=\"message\">\n        <PxAlert v-bind:text=\"text\" v-bind:asset=\"asset\"/>\n        </div>\n        <div v-if=\"message_increment\">\n          <PxAlert v-bind:text_increment=\"text_increment\" v-bind:asset=\"asset\"/>\n        </div>\n\n      </div>\n    </div>\n      <!-- pregunto si hay algun dato a tener -->\n      <div v-if=\"Object.entries(asset)\">\n          <PxDetalle v-bind:asset=\"asset\" v-bind:isLoading=\"isLoading\"  /> \n      </div>\n\n      \n      <PxFooter/>\n      \n\n      \n    </v-main>\n  </v-app>\n</template>\n\n<script>\n\nimport PxAlert from '@/components/PxAlert';\nimport PxPaginacion from '@/components/PxPaginacion';\nimport PxDetalle from '@/components/PxDetalle';\n\nimport PxFooter from '@/components/PxFooter';\nimport app from \"@/app\";\nexport default {\n  name: 'App',\n\n  components: {\n    PxAlert,PxPaginacion,PxDetalle,PxFooter,\n  },\n  data() {\n    return{\n      // isLoading: false,\n      asset: {},\n      pokemonID:\"\",\n      cant:1,\n      message:false,\n      message_increment:false,\n      text_increment:\"es el ultimo pokemon de la lista. No se puede seguir hacia adelante. Seleccione el botón anterior.\",\n      text:\"es el ultimo pokemon de la lista. No se puede seguir hacia atrás. Seleccione el botón siguiente.\",\n      isLoading: false,\n  \n    }\n    \n  },\n  created() {\n    this.getCoin();\n  },\n\n  methods: {\n        increment() {\n\n            this.message=false\n            this.message_increment=false\n            this.cant=this.cant+1\n            const imprimir=this.cant\n            console.log(imprimir)\n            \n            if(this.cant>8 ){\n              this.cant=8\n              this.message_increment=true\n            }\n            if(this.cant>0){\n              this.getCoin(this.cant)\n            }\n        },\n        decrement() {\n\n          this.message_increment=false\n          this.cant=this.cant-1\n          const imprimir=this.cant\n            console.log(imprimir)\n            if(this.cant<1){\n              this.cant=1\n              this.message=true\n            }\n            if(this.cant>0){\n              this.getCoin(this.cant)\n            }\n              \n        },\n        getCoin() {\n          this.isLoading=true\n          Promise.all([app.getPokemon(this.cant)])\n            .then(([asset]) => {\n              this.asset = asset;\n              // this.history = history;\n            })\n            .finally(() => (this.isLoading = false));\n       },\n       getBusqueda() {\n          this.isLoading=true\n          Promise.all([app.getPokemon(this.pokemonID)])\n            .then(([asset]) => {\n              this.asset = asset;\n              // this.history = history;\n            })\n            .finally(() => (this.isLoading = false));\n       }\n    },\n  \n};\n</script>\n<style>\n\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  }\n  /* input */\n  /* caja a completar */\n  .pokemon-form{\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    /* background-image: url(\"assets/header_pokemon.png\"); */\n    background-color:red ;\n    padding-top:2rem;\n    padding-bottom: 2rem;\n  }\n  .pokemon-label{\n    background-color: red;\n    color:white;\n    display: inline-block;\n  }\n  input[type=text], select {\n  width: 25%;\n  padding: 12px 20px;\n  margin: 8px 0;\n  display: inline-block;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  box-sizing: border-box;\n  color: white;\n}\n/* mensajes de error */\n.content_alert{\n    position:absolute;\n    /* margin-top: 0; */\n    /* margin-left: 33rem; */\n    position: absolute; \n  left: 0; \n  right: 0; \n  margin-left: auto; \n  margin-right: auto; \n  width: 70%; /* Need a specific value to work */\n\n  }\n  .alinear{\n    justify-content: center;\n  }\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}