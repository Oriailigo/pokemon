profile {\n        background: white;\n        border-radius: 50px;\n        padding: 1.5em 1.75em;\n        margin: 2em auto;\n        max-width: 330px;\n        box-shadow: 0px 3px 136px 0px rgba(88, 87, 119, 0.21);\n    }\n    .img-background{\n      background-color: black;\n    }\n.card-tile-margin{\n  margin-top: -5rem;\n}\n.card-title{\n  font-size: 1.8rem;\n  text-transform: capitalize;\n  color: hsl(0deg 0% 59%);\n  font-weight: 400;\n}\n/* card footer     */\n.card-footer{\n    display: flex;\n    flex-direction: row;\ngap: 2rem;\n}\n.card-footer-social{\n    margin-top: 2rem;\n    margin-bottom: 2rem;\n}\n.card-footer-social h3{\n  font-weight: bolder;\n  color:black\n}\n/* foto */\n.body__profile__photo img {\n        border-radius: 50%;\n        width: 200px;\n        height: 200px;\n        border: 15px solid #fff;\n        top: -100px;\n        position: relative;\n        box-shadow: 1px 2px 18px 4px rgb(226, 226, 226);\n    }\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}