{"ast":null,"code":"import HelloWorld from '@/components/HelloWorld';\nimport PxAlert from '@/components/PxAlert';\nimport PxPaginacion from '@/components/PxPaginacion';\nimport PxDetalle from '@/components/PxDetalle';\nimport PxFooter from '@/components/PxFooter';\nimport app from \"@/app\";\nexport default {\n  name: 'App',\n  components: {\n    HelloWorld,\n    PxAlert,\n    PxPaginacion,\n    PxDetalle,\n    PxFooter\n  },\n  data() {\n    return {\n      // isLoading: false,\n      asset: {},\n      cant: 1,\n      message: false,\n      message_increment: false,\n      text_increment: \"es el ultimo pokemon de la lista. No se puede seguir hacia adelante. Seleccione el botón anterior.\",\n      text: \"es el ultimo pokemon de la lista. No se puede seguir hacia atrás. Seleccione el botón siguiente.\",\n      isLoading: false\n    };\n  },\n  created() {\n    this.getCoin();\n  },\n  methods: {\n    increment() {\n      this.message = false;\n      this.message_increment = false;\n      this.cant = this.cant + 1;\n      const imprimir = this.cant;\n      console.log(imprimir);\n      if (this.cant > 8) {\n        this.cant = 8;\n        this.message_increment = true;\n      }\n      if (this.cant > 0) {\n        this.getCoin(this.cant);\n      }\n    },\n    decrement() {\n      this.message_increment = false;\n      this.cant = this.cant - 1;\n      const imprimir = this.cant;\n      console.log(imprimir);\n      if (this.cant < 1) {\n        this.cant = 1;\n        this.message = true;\n      }\n      if (this.cant > 0) {\n        this.getCoin(this.cant);\n      }\n    },\n    getCoin() {\n      this.isLoading = true;\n\n      // const id = this.$route.params.id;\n      // var id=this.cant\n      Promise.all([app.getPokemon(this.cant)]).then(([asset]) => {\n        this.asset = asset;\n        // this.history = history;\n      }).finally(() => this.isLoading = false);\n    }\n  }\n};","map":{"version":3,"mappings":"AAyCA;AACA;AACA;AACA;AAEA;AACA;AACA;EACAA;EAEAC;IACAC;IAAAC;IAAAC;IAAAC;IAAAC;EACA;EACAC;IACA;MACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;IAEA;EAEA;EACAC;IACA;EACA;EAEAC;IACAC;MAEA;MACA;MACA;MACA;MACAC;MAEA;QACA;QACA;MACA;MACA;QACA;MACA;IACA;IACAC;MAEA;MACA;MACA;MACAD;MACA;QACA;QACA;MACA;MACA;QACA;MACA;IAEA;IACAE;MACA;;MAEA;MACA;MACAC,yCACAC;QACA;QACA;MACA,GACAC;IACA;EACA;AAEA","names":["name","components","HelloWorld","PxAlert","PxPaginacion","PxDetalle","PxFooter","data","asset","cant","message","message_increment","text_increment","text","isLoading","created","methods","increment","console","decrement","getCoin","Promise","then","finally"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-main>\n      <HelloWorld/>\n      <form class=\"pokemon-form\">\n          <input\n            class=\"pokemon-input\"\n            type=\"number\"\n            name=\"pokemon\"\n            v-model=\"cant\"\n            placeholder=\"Busca tu pokemon\"\n            autocomplete=\"off\"/>\n          <button\n            @click=\"getCoin\"\n            type=\"submit\"\n            class=\"pokemon-btn\"\n            value=\"\"\n          >\n          buscar pokemon\n          </button>\n      </form>\n\n      <PxPaginacion  v-bind:increment=\"increment\" v-bind:decrement=\"decrement\" v-bind:cant=\"cant\"/>\n      <!-- pregunto si hay algun dato a tener -->\n      <div v-if=\"Object.entries(asset)\">\n          <PxDetalle v-bind:asset=\"asset\" v-bind:isLoading=\"isLoading\"  /> \n      </div>\n      <div v-if=\"message\">\n        <PxAlert v-bind:text=\"text\" v-bind:asset=\"asset\"/>\n      </div>\n      <div v-if=\"message_increment\">\n        <PxAlert v-bind:text_increment=\"text_increment\" v-bind:asset=\"asset\"/>\n      </div>\n\n      <PxFooter/>\n      \n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport HelloWorld from '@/components/HelloWorld';\nimport PxAlert from '@/components/PxAlert';\nimport PxPaginacion from '@/components/PxPaginacion';\nimport PxDetalle from '@/components/PxDetalle';\n\nimport PxFooter from '@/components/PxFooter';\nimport app from \"@/app\";\nexport default {\n  name: 'App',\n\n  components: {\n    HelloWorld,PxAlert,PxPaginacion,PxDetalle,PxFooter,\n  },\n  data() {\n    return{\n      // isLoading: false,\n      asset: {},\n      cant:1,\n      message:false,\n      message_increment:false,\n      text_increment:\"es el ultimo pokemon de la lista. No se puede seguir hacia adelante. Seleccione el botón anterior.\",\n      text:\"es el ultimo pokemon de la lista. No se puede seguir hacia atrás. Seleccione el botón siguiente.\",\n      isLoading: false,\n  \n    }\n    \n  },\n  created() {\n    this.getCoin();\n  },\n\n  methods: {\n        increment() {\n\n            this.message=false\n            this.message_increment=false\n            this.cant=this.cant+1\n            const imprimir=this.cant\n            console.log(imprimir)\n            \n            if(this.cant>8 ){\n              this.cant=8\n              this.message_increment=true\n            }\n            if(this.cant>0){\n              this.getCoin(this.cant)\n            }\n        },\n        decrement() {\n\n          this.message_increment=false\n          this.cant=this.cant-1\n          const imprimir=this.cant\n            console.log(imprimir)\n            if(this.cant<1){\n              this.cant=1\n              this.message=true\n            }\n            if(this.cant>0){\n              this.getCoin(this.cant)\n            }\n              \n        },\n        getCoin() {\n          this.isLoading=true\n\n        // const id = this.$route.params.id;\n        // var id=this.cant\n          Promise.all([app.getPokemon(this.cant)])\n            .then(([asset]) => {\n              this.asset = asset;\n              // this.history = history;\n            })\n            .finally(() => (this.isLoading = false));\n       }\n    },\n  \n};\n</script>\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}