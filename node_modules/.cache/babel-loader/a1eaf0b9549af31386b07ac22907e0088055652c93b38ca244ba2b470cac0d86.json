{"ast":null,"code":"import HelloWorld from '@/components/HelloWorld';\nimport PxPaginacion from '@/components/PxPaginacion';\nimport PxDetalle from '@/components/PxDetalle';\nimport app from \"@/app\";\nexport default {\n  name: 'App',\n  components: {\n    HelloWorld,\n    PxPaginacion,\n    PxDetalle\n  },\n  data() {\n    return {\n      // isLoading: false,\n      asset: {},\n      cant: 1,\n      message: false,\n      message_increment: false\n    };\n  },\n  created() {\n    this.getCoin();\n  },\n  methods: {\n    increment() {\n      this.message = false;\n      this.message_increment = false;\n      this.cant = this.cant + 1;\n      const imprimir = this.cant;\n      console.log(imprimir);\n      if (this.cant > 8) {\n        this.cant = 8;\n        this.message_increment = true;\n      }\n      if (this.cant > 0) {\n        this.getCoin(this.cant);\n      }\n    },\n    decrement() {\n      this.message_increment = false;\n      this.cant = this.cant - 1;\n      const imprimir = this.cant;\n      console.log(imprimir);\n      if (this.cant < 1) {\n        this.cant = 1;\n        this.message = true;\n      }\n      if (this.cant > 0) {\n        this.getCoin(this.cant);\n      }\n    },\n    getCoin() {\n      // this.isLoading = true;\n      // const id = this.$route.params.id;\n      // var id=this.cant\n      Promise.all([app.getPokemon(this.cant)]).then(([asset]) => {\n        this.asset = asset;\n        // this.history = history;\n      });\n    }\n  }\n\n  // created() {\n  //   // this.isLoading = true;\n  //   // app\n  //   //   .getPokemons()\n  //   //   .then((assets) => (console.log(assets)))\n  //     // .finally(() => (this.isLoading = false));\n  //   // console.log(this.assets);\n  //   app.fetchData()\n  //   // app.getPokemon(1)\n  //   .then((asset)=>(this.assets=asset))\n  //   .then((asset)=>(this.cantidad=asset.id))\n\n  // },\n};","map":{"version":3,"mappings":"AAyBA;AACA;AACA;AACA;AACA;EACAA;EAEAC;IACAC;IAAAC;IAAAC;EACA;EACAC;IACA;MACA;MACAC;MACAC;MACAC;MACAC;IAEA;EACA;EACAC;IACA;EACA;EAEAC;IACAC;MACA;MACA;MACA;MACA;MACAC;MAEA;QACA;QACA;MACA;MACA;QACA;MACA;IACA;IACAC;MACA;MACA;MACA;MACAD;MACA;QACA;QACA;MACA;MACA;QACA;MACA;IAEA;IACAE;MACA;MACA;MACA;MACAC,yCACAC;QACA;QACA;MACA;IACA;EACA;;EAEA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;;EAEA;AAEA","names":["name","components","HelloWorld","PxPaginacion","PxDetalle","data","asset","cant","message","message_increment","created","methods","increment","console","decrement","getCoin","Promise","then"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-main>\n      <HelloWorld/>\n\n      <PxPaginacion  v-bind:increment=\"increment\" v-bind:decrement=\"decrement\" v-bind:cant=\"cant\"/>\n      <PxDetalle v-bind:asset=\"asset\"   /> \n      <div>\n        <v-alert v-if=\"message\"\n            title=\"No se puede seguir hacia atras.\"\n            text=\"Lorem ipsum dolor sit amet consectetur adipisicing elit. Commodi, ratione debitis quis est labore voluptatibus! Eaque c\"\n            type=\"error\"\n            variant=\"tonal\"\n        ></v-alert>\n        <h1 v-if=\"message\">{{asset.name}} es el primero de la lista. Intente hacia el siguiente.</h1></div>\n      <div><h1 v-if=\"message_increment\">{{asset.name}} es el ultimo pokemon de la lista. Intente hacia el anterior.</h1></div>\n      <!-- <p>{{assets.name}}</p> -->\n\n\n\n    </v-main>\n  </v-app>\n</template>\n\n<script>\nimport HelloWorld from '@/components/HelloWorld';\nimport PxPaginacion from '@/components/PxPaginacion';\nimport PxDetalle from '@/components/PxDetalle';\nimport app from \"@/app\";\nexport default {\n  name: 'App',\n\n  components: {\n    HelloWorld,PxPaginacion,PxDetalle\n  },\n  data() {\n    return{\n      // isLoading: false,\n      asset: {},\n      cant:1,\n      message:false,\n      message_increment:false\n      \n    }\n  },\n  created() {\n    this.getCoin();\n  },\n\n  methods: {\n        increment() {\n            this.message=false\n            this.message_increment=false\n            this.cant=this.cant+1\n            const imprimir=this.cant\n            console.log(imprimir)\n            \n            if(this.cant>8 ){\n              this.cant=8\n              this.message_increment=true\n            }\n            if(this.cant>0){\n              this.getCoin(this.cant)\n            }\n        },\n        decrement() {\n          this.message_increment=false\n          this.cant=this.cant-1\n          const imprimir=this.cant\n            console.log(imprimir)\n            if(this.cant<1){\n              this.cant=1\n              this.message=true\n            }\n            if(this.cant>0){\n              this.getCoin(this.cant)\n            }\n              \n        },\n        getCoin() {\n        // this.isLoading = true;\n        // const id = this.$route.params.id;\n        // var id=this.cant\n          Promise.all([app.getPokemon(this.cant)])\n            .then(([asset]) => {\n              this.asset = asset;\n              // this.history = history;\n            })\n       }\n    },\n  \n  // created() {\n  //   // this.isLoading = true;\n  //   // app\n  //   //   .getPokemons()\n  //   //   .then((assets) => (console.log(assets)))\n  //     // .finally(() => (this.isLoading = false));\n  //   // console.log(this.assets);\n  //   app.fetchData()\n  //   // app.getPokemon(1)\n  //   .then((asset)=>(this.assets=asset))\n  //   .then((asset)=>(this.cantidad=asset.id))\n\n  // },\n  \n};\n</script>\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n/* card */\n.body__container { \n        margin: 0 auto;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n    }\n    .body__profile {\n        background: white;\n        border-radius: 50px;\n        padding: 1.5em 1.75em;\n        margin: 2em auto;\n        max-width: 330px;\n        box-shadow: 0px 3px 136px 0px rgba(88, 87, 119, 0.21);\n    }\n    .img-background{\n      background-color: black;\n    }\n.card-tile-margin{\n  margin-top: -5rem;\n}\n.card-title{\n  font-size: 1.8rem;\n  text-transform: capitalize;\n  color: hsl(0deg 0% 59%);\n  font-weight: 400;\n}\n/* card footer     */\n.card-footer{\n    display: flex;\n    flex-direction: row;\ngap: 2rem;\n}\n.card-footer-social{\n    margin-top: 2rem;\n    margin-bottom: 2rem;\n}\n.card-footer-social h3{\n  font-weight: bolder;\n  color:black\n}\n/* foto */\n.body__profile__photo img {\n        border-radius: 50%;\n        width: 200px;\n        height: 200px;\n        border: 15px solid #fff;\n        top: -100px;\n        position: relative;\n        box-shadow: 1px 2px 18px 4px rgb(226, 226, 226);\n    }\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}